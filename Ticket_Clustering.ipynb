    "try:\n",
    "    nltk.data.find(\"corpora/stopwords\")\n",
    "except LookupError as e:\n",
    "    print(repr(e))\n",
    "    nltk.download('stopwords')\n",
    "\n",
    "def cleanData(data, stopWords):\n",
    "    formatStopWords = r'\\b(?:{})\\b'.format('|'.join(stopWords))\n",
    "    data['cleantext'] = data['cleantext'].str.replace(\"[^a-zA-Z#]\", \" \")\n",
    "    data['cleantext'] = data['cleantext'].str.lower().replace([r'\\|', formatStopWords],[\"\\n\",\"\"], regex=True)\n",
    "    data['cleantext'] = data['cleantext'].str.replace(\"\\r|\\n\",\" \")\n",
    "    data['cleantext'] = data['cleantext'].str.replace(\"  \",\" \")\n",
    "    data['cleantext'] = data['cleantext'].str.strip()\n",
    "    return data;\n",
    "\n",
    "def lemmatize_text(text):\n",
    "    w_tokenizer = nltk.tokenize.WhitespaceTokenizer()\n",
    "    lemmatizer = nltk.stem.WordNetLemmatizer()\n",
    "    if text is not None:\n",
    "        return \" \".join([lemmatizer.lemmatize(w) for w in w_tokenizer.tokenize(str(text))])\n",
    "        #return [lemmatizer.lemmatize(w) for w in w_tokenizer.tokenize(str(text))]\n",
    "\n",
    "def stemm_text(text):\n",
    "    w_tokenizer = nltk.tokenize.WhitespaceTokenizer()\n",
    "    porter_stemmer  = PorterStemmer()\n",
    "    #print(\" \".join([porter_stemmer.stem(w) for w in w_tokenizer.tokenize(text)]))\n",
    "    return \" \".join([porter_stemmer.stem(w) for w in w_tokenizer.tokenize(text)])\n",
    "\n",
    "def createStopWords():\n",
    "    #from nltk.corpus import stopwords\n",
    "    stopWords = stopwords.words('english')\n",
    "    newStopWords=['customer', 'please']\n",
    "    \n",
    "    stopWords.extend(newStopWords)\n",
    "    stopWords.remove('not')\n",
    "    stopWords.remove('down')\n",
    "    return stopWords\n",
    "\n",
    "def createCleanText(data):\n",
    "    data = data[data.Number.notna()]\n",
    "    #data.shape\n",
    "    data.set_index('Number', inplace = True)\n",
    "    #frequentlines(data,'short_description')\n",
    "    data['cleantext'] = data['final_description']\n",
    "    stopwords=createStopWords()\n",
    "    data = cleanData(data, stopwords)\n",
    "    return data"
   ]
